{
	"workbench.iconTheme": "material-icon-theme",
	"editor.suggestSelection": "first",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	// カラーピッカー
	"editor.colorDecorators": true,
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"[cpp]": {
		// "editor.defaultFormatter": "xaver.clang-format"
	},
	// "editor.fontLigatures": null,
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"vsicons.dontShowNewVersionMessage": true,
	"editor.formatOnSave": true,
	// Enable per-language basis by scoping for Prettier
	"editor.defaultFormatter": "biomejs.biome",
	// JavaScript関連
	"[javascript]": {
		"editor.tabSize": 2,
		"editor.rulers": [80],
		"editor.insertSpaces": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.tabSize": 2,
		"editor.rulers": [80],
		"editor.insertSpaces": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.formatOnSave": true,
		"editor.rulers": [80]
	},
	// TypeScript の場合は以下も
	"[typescript]": {
		"editor.tabSize": 2,
		"editor.rulers": [80],
		"editor.insertSpaces": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.tabSize": 2,
		"editor.rulers": [80],
		"editor.insertSpaces": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.tabSize": 2,
		"editor.rulers": [80],
		"editor.insertSpaces": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.tabSize": 2,
		"editor.rulers": [80],
		"editor.insertSpaces": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[scss]": {
		"editor.tabSize": 2,
		"editor.rulers": [80]
	},
	// GO関連
	"[go]": {
		"editor.tabSize": 4,
		"editor.insertSpaces": false,
		"editor.formatOnSave": true,
		"editor.formatOnPaste": false,
		"editor.formatOnType": false,
		"editor.defaultFormatter": "golang.go"
	},
	// Flutter関連
	"[dart]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [80],
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": false,
		"editor.suggestSelection": "first",
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": "off",
		"editor.defaultFormatter": "Dart-Code.dart-code",
		"editor.formatOnPaste": true
	},
	"editor.formatOnType": true,
	"terminal.integrated.profiles.linux": {
		"bash": {
			"path": "bash",
			"icon": "terminal-bash"
		},
		"zsh": {
			"path": "zsh"
		},
		"fish": {
			"path": "fish"
		},
		"tmux": {
			"path": "tmux",
			"icon": "terminal-tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		}
	},
	"emmet.includeLanguages": {
		"django-html": "html"
	},
	"[django-html]": {
		"editor.tabSize": 4,
		"editor.formatOnSave": false
		// "editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"html.format.wrapLineLength": 140,
	"[makefile]": {
		"editor.insertSpaces": false
	},
	"files.trimTrailingWhitespace": true, // 末尾の空白を自動削除
	"files.insertFinalNewline": true,
	// vim 設定
	"vim.useSystemClipboard": true, // OS のクリップボードと同期
	"vim.foldfix": true, // 折りたたみ箇所を開かないように変更
	"vim.highlightedyank.enable": true,
	"vim.highlightedyank.color": "rgba(100, 170, 240, 0.8)",
	"vim.highlightedyank.duration": 250,
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindings": [
		// カーソル移動
		{
			"before": ["J"],
			"after": ["5", "j"]
		},
		{
			"before": ["K"],
			"after": ["5", "k"]
		},
		{
			"before": ["L"],
			"after": ["$"]
		},
		{
			"before": ["H"],
			"after": ["^"]
		}
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [">"],
			"after": [">", "g", "v"]
		},
		{
			"before": ["<"],
			"after": ["<", "g", "v"]
		},
		// カーソル移動
		{
			"before": ["J"],
			"after": ["5", "j"]
		},
		{
			"before": ["K"],
			"after": ["5", "k"]
		}
	],
	// モード表示を有効にする設定
	"vim.showmodename": true,
	// オプション：モード表示の位置を左に設定
	"vim.statusBarPosition": "left",
	// 検索にマッチした箇所をハイライト
	"vim.hlsearch": true,
	"editor.minimap.size": "fit",
	"editor.fontWeight": 450,
	"editor.fontFamily": "JetBrains Mono, HackGenNerd",
	"editor.inlayHints.fontFamily": "JetBrains Mono, HackGenNerd",
	"scm.inputFontFamily": "JetBrains Mono, HackGenNerd",
	"editor.codeLensFontFamily": "JetBrains Mono, HackGenNerd",
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.format.enable": false,
	"eslint.enable": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.tabSize": 2,
	// Prettierのフォーマット設定
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"debug.console.fontFamily": "JetBrains Mono, HackGenNerd",
	"scm.inputFontSize": 13,
	"terminal.integrated.fontWeight": "normal",
	"settingsSync.ignoredExtensions": [],
	"editor.quickSuggestionsDelay": 1,
	"remote.SSH.remotePlatform": {
		"Test-EC2": "linux"
	},
	"workbench.startupEditor": "none",
	"files.exclude": {
		"": true,
		"**/.git": false
	},
	"extensions.autoUpdate": "onlyEnabledExtensions",
	"prettier.requireConfig": true,
	// ファイルツリー
	"workbench.tree.indent": 22,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.colorCustomizations": {
		"editorBracketHighlight.foreground1": "#FFD700",
		"editorBracketHighlight.foreground2": "#1E90FF",
		"editorBracketHighlight.foreground3": "#DDA0DD",
		"editorBracketHighlight.foreground4": "#3CB371",
		"editorBracketHighlight.foreground5": "#FF5F5F",
		"editorBracketHighlight.unexpectedBracket.foreground": "#FF5F5F"
	},
