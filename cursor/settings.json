	"todo-tree.general.tags": [
		"CHANGED",
		"FIXME",
		"NOTE",
		"REVIEW",
		"TODO",
		"WARNING"
	],
	"todo-tree.highlights.defaultHighlight": {
		"icon": "checklist",
		"type": "text",
		"fontWeight": "normal",
		"iconColour": "#2C7CFF"
	},
	"todo-tree.highlights.customHighlight": {
		"CHANGED": {
			"foreground": "#FFFFFF",
			"background": "#E54F6D"
		},
		"FIXME": {
			"foreground": "#FFFFFF",
			"background": "#2986FF"
		},
		"NOTE": {
			"foreground": "#FFFFFF",
			"background": "#3A3A3A"
		},
		"REVIEW": {
			"foreground": "#FFFFFF",
			"background": "#FF00FF"
		},
		"TODO": {
			"foreground": "#FFFFFF",
			"background": "#319200"
		},
		"WARNING": {
			"foreground": "#000000",
			"background": "#FFBF00"
		}
	},
	"better-comments.tags": [
		{
			"tag": "CHANGED",
			"strikethrough": false,
			"backgroundColor": "transparent",
			"foregroundColor": "#FFFFFF"
			// "color": "#EA7FA5"
		},
		{
			"tag": "FIXME",
			"strikethrough": false,
			"backgroundColor": "transparent",
			"foregroundColor": "#FFFFFF"
			// "color": "#2986FF"
		},
		{
			"tag": "NOTE",
			"strikethrough": false,
