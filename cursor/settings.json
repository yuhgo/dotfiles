		"**/app/**/page.tsx": "${dirname} - Page",
		"**/app/**/layout.tsx": "${dirname} - Layout",
		"**/app/**/action.tsx": "${dirname} - Action",
		"**/app/**/loading.tsx": "${dirname} - Loading",
		"**/app/**/error.tsx": "${dirname} - Error",
		"**/app/**/default.tsx": "${dirname} - Default",
		"**/app/**/modal.tsx": "${dirname} - Modal",
		// "**/app/**/_component/**/*.tsx": "${dirname(1)}/${dirname(0)}/${filename} - Comp",
		"**/app/**/_component/**/*.tsx": "${filename}.${extname} - Comp",
		"**/app/**/_lib/**/*.ts": "${dirname(0)}/${filename}.${extname} - Lib",
		"**/app/**/_lib/**/*.tsx": "${dirname(0)}/${filename}.${extname} - Lib",
		// index.tsxファイルがわかりやすく
		"**/src/**/index.tsx": "${dirname(1)}/${dirname(0)} - index",
		"**/src/**/*.tsx": "${dirname(0)} - ${filename}.${extname}",
		"**/src/**/*.ts": "${dirname(0)} - ${filename}.${extname}"
	},
